class Solution {
    public int evalRPN(String[] tokens) {
        Stack<Integer> stack = new Stack<>();
        Integer a,b;
        for(String s:tokens){
            switch(s){
                case "+":
                    stack.push(stack.pop()+stack.pop());
                    break;
                case "-":
                    a=stack.pop();b=stack.pop();
                    stack.push(b-a);
                    break; 
                case "*":
                    a=stack.pop();b=stack.pop();
                    stack.push(a*b);
                    break;
                case "/":
                    a=stack.pop();b=stack.pop();
                    stack.push(b/a);
                    break;
                default:
                    stack.push(Integer.valueOf(s));
                    break;
            }
        }
        return stack.pop();
    }
}
